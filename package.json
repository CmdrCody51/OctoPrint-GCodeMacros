{
  "name": "octoprint-gcode-macros",
  "version": "1.0.0",
  "description": "Configure custom gcode macros for maximum productivity!",
  "scripts": {
    "css-dev": "sass --watch octoprint_gcode_macro/static/src/gcode_macro.scss octoprint_gcode_macro/static/dist/gcode_macro.css",
    "css-build": "sass --style=compressed --no-source-map octoprint_gcode_macro/static/src/gcode_macro.scss octoprint_gcode_macro/static/dist/gcode_macro.css",
    "release": "npm run css-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cp2004/OctoPrint-GCodeMacros.git"
  },
  "author": "Charlie Powell",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/cp2004/OctoPrint-GCodeMacros/issues"
  },
  "homepage": "https://github.com/cp2004/OctoPrint-GCodeMacros#readme",
  "devDependencies": {
    "sass": "^1.53.0"
  }
}
